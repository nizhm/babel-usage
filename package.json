{
  "name": "babel-usage",
  "version": "1.0.0",
  "description": "Learning how to use babel",
  "main": "index.js",
  "scripts": {
    "babel:help": "babel --help",
    "babel": "babel --config-file ./babel.config.js src -d dist",
    "babel:arrow": "babel ./src --out-dir ./dist --config-file ./babel.arrow.config.js",
    "babel:rollup": "rollup --config",
    "babel:rollup:multiple": "node multiple-transform.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nizhm/babel-usage.git"
  },
  "keywords": [
    "babel"
  ],
